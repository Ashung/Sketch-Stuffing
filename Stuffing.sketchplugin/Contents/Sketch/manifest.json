                    "SupplyPhoneNumbers": "onSupplyPhoneNumbers",
                    "SupplyCarNumbers": "onSupplyCarNumbers",
                    "SupplyIdCardNumbers": "onSupplyIdCardNumbers",

                    "SupplyTextFromFile": "onSupplyTextFromFile",
                    "SupplyImageFromFolder": "onSupplyImageFromFolder",
