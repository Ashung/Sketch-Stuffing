{
    "name" : "Stuffing",
    "description" : "Sketch data supplier plugin.",
    "author" : "Ashung Hung",
    "authorEmail" : "ashung.hung@foxmail.com",
    "homepage" : "https://github.com/Ashung/Stuffing",
    "identifier" : "com.ashung.hung.stuffing",
