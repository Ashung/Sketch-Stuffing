{
    "name" : "Stuffing",
    "description" : "Sketch data supplier plugin for Chinese.",
    "author" : "Ashung Hung",
    "authorEmail" : "ashung.hung@foxmail.com",
    "homepage" : "https://github.com/Ashung/Sketch-Stuffing",
    "identifier" : "com.ashung.hung.stuffing",
    "suppliesData" : true,
    "version" : "1.2.0",
    "icon": "icon.png",
