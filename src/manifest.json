                    "SupplyProvinces": "onSupplyProvinces",
                    "SupplyCities": "onSupplyCities",
                    "SupplyProvinceAndCities": "onSupplyProvinceAndCities",
                    "SupplyMobileNumbers": "onSupplyMobileNumbers",
                    "SupplyPhoneNumbers": "onSupplyPhoneNumbers",
                    "SupplyCarNumbers": "onSupplyCarNumbers",
                    "SupplyIdCardNumbers": "onSupplyIdCardNumbers",
                    "SupplyCustomNumbers": "onSupplyCustomNumbers",
                    "SupplySequenceNumbers": "onSupplySequenceNumbers",
                    
                    "SupplyTextFromFile": "onSupplyTextFromFile",
                    "SupplyImageFromFolder": "onSupplyImageFromFolder",
                    "SupplyRandomTextFromFile": "onSupplyRandomTextFromFile"
